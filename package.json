{
  "name": "vomu-backend",
  "version": "1.0.0",
  "description": "",
  "main": "dist/server",
  "scripts": {
    "start": "node dist/server.js",
    "watch:ts": "tsc -w",
    "watch:node": "nodemon --inspect -r dotenv/config --watch dist",
    "watch": "concurrently -k -p \"[{name}]\" -n \"Node,TypeScript\" -c \"yellow.bold,blue.bold\" \"yarn watch:node\" \"yarn watch:ts\"",
    "build": "tsc",
    "lint": "eslint \"src/**/*.ts\" --quiet --fix",
    "migrate": "migrate-mongo up",
    "migrate_rollback": "migrate-mongo down"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ITESM-DW/vomu-backend.git"
  },
  "author": "kevinwkt",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ITESM-DW/vomu-backend/issues"
  },
  "homepage": "https://github.com/ITESM-DW/vomu-backend#readme",
  "dependencies": {
		"@types/bcrypt": "^3.0.0",
		"@types/body-parser": "^1.19.0",
		"@types/connect-timeout": "^0.0.34",
		"@types/eslint": "^6.8.1",
		"@types/express": "^4.17.6",
		"@types/express-session": "^1.17.0",
		"@types/jsonwebtoken": "^8.5.0",
		"@types/mongodb": "^3.5.20",
		"@types/mongoose": "^5.7.23",
		"@types/morgan": "^1.9.0",
		"@types/passport": "^1.0.3",
		"@types/passport-jwt": "^3.0.3",
		"@types/passport-local": "^1.0.33",
		"@types/yup": "^0.29.2",
		"@typescript-eslint/eslint-plugin": "^3.2.0",
		"@typescript-eslint/parser": "^3.2.0",
    "bcrypt": "^5.0.0",
    "bson": "^4.0.4",
    "connect-timeout": "^1.9.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.16",
    "morgan": "^1.10.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "yup": "^0.29.1"
  },
  "devDependencies": {
    "dotenv": "^8.2.0",
    "eslint": "^7.2.0",
    "migrate-mongo": "^7.2.1",
    "nodemon": "^2.0.4",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5"
  }
}
